{
  "manifest_version": 2,
  "name": "Word-Replace",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.jpg"
  },
  "icons": {
    "128": "logo.jpg"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
